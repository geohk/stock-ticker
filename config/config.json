{
    "yahoo_api": {
        "endpoint": "https://query1.finance.yahoo.com/v7/finance/quote?lang=en-US&region=US&corsDomain=finance.yahoo.com",
        "fields": [
            "symbol",
            "marketState",
            "regularMarketPrice",
            "regularMarketChange",
            "regularMarketChangePercent",
            "preMarketPrice",
            "preMarketChange",
            "preMarketChangePercent",
            "postMarketPrice",
            "postMarketChange",
            "postMarketChangePercent"
        ]
    },
    "refresh_frequency": 5000,
    "sync_period": {
        "start_at": "6:00",
        "end_at": "14:00"
    }
}